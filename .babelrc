{
    "presets": [
      ["@babel/preset-env",
      {
          "targets": {
            "node": "14.18.1"
          }
        }]
    ],
    "plugins": [
      [
        "add-module-exports"
      ],
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
}